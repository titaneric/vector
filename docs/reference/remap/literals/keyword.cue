	examples: [
		"if",
		"else",
		"for",
		"while",
		"loop",
		"abort",
		"break",
		"continue",
		"return",
		"as",
		"type",
		"let",
		"until",
		"then",
		"impl",
		"in",
		"self",
		"this",
		"use",
		"std",
		"null",
		"true",
		"false",
	]
}
