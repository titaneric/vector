<% if array %>[[<%= path %>]]<% else %>[<%= path %>]<% end %>
<% example.grouped.each do |title, options| -%>
  <%- if example.grouped.length > 1 -%>
  # <%= title %>
  <%- end -%>
  <%- options.each do |option| -%>
    <%- if option.array?("table") -%>
<%= config_example(option.options, array: true, path: "#{path}.#{option.name}", simple: simple).indent(2) %>
    <%- elsif option.table? -%>
<%= config_example(option.options, path: "#{path}.#{option.name}", simple: simple).indent(2) %>
    <%- else -%>
      <%- if option.wildcard? -%>
        <%- option.examples.each do |e| -%>
  <%= e.fetch("name") %> = <%= e.fetch("value").to_toml %> # example
        <%- end -%>
      <%- else -%>
        <%- first_example = option.examples.first -%>
        <%- tags = option_tags(option, default: :short, example: false, optionality: false, type: false, unit: :short) -%>
        <%- if first_example.is_a?(Hash) -%>
  <%= first_example.fetch("name") %> = <%= first_example.fetch("value").to_toml %>
        <%- else -%>
  <%= option.name %> = <%= first_example.to_toml %><% if tags.any? && !first_example.to_toml.include?("\n") %> # <%= tags.join(", ") %><% end %>
        <%- end -%>
      <%- end -%>
    <%- end -%>
  <%- end -%>
  <%- if titles -%>
  
  <%- end -%>
<%- end -%>