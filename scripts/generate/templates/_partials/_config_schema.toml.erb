[<%= opts[:path] %>]
<% schema.grouped.each do |title, options| -%>
<% if opts[:titles] -%>
  # <%= title %>
<% end -%>
<% options.each do |option| -%>
<% if option.table? -%>
<%= config_schema(option.options, titles: false, path: "#{opts[:path]}.#{option.name}").indent(2) %>
<% else -%>
  <%= option.name %> = <%= schema.type_string(option.type, option.enum) %>
<% end -%>
<% end %>
<% end -%>